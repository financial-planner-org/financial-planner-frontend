{
    "version": 2,
    "buildCommand": "npm run build",
    "outputDirectory": ".next",
    "framework": "nextjs",
    "installCommand": "npm install",
    "devCommand": "npm run dev",
    "build": {
        "env": {
            "NODE_ENV": "production"
        }
    },
    "git": {
        "deploymentEnabled": {
            "main": true,
            "fix/vercel-build": true
        }
    },
    "functions": {
        "src/app/api/**/*.ts": {
            "runtime": "nodejs18.x"
        }
    }
}